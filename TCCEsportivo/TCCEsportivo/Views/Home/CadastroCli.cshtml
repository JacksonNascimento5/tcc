@model TCCEsportivo.Models.DTO
@{
    ViewBag.Title = "CadastroCli";
}
<h2>Cadastro do cliente</h2>

@using (Html.BeginForm("ObrigadoCliente", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    @Html.ValidationSummary()

    <fieldset class="">
        

        <div class="editor-label">
            @Html.LabelFor(m => m.NomeCli)
        </div>
        <div class="editor-field">
            @Html.EditorFor(m => m.NomeCli)
        </div>
        <div class="editor-label">
            @Html.LabelFor(m => m.CpfCli)
        </div>
        <div class="editor-field">
            @Html.EditorFor(m => m.CpfCli)
        </div>
        <div class="editor-label">
            @Html.LabelFor(m => m.EmailCli)
        </div>
        <div class="editor-field">
            @Html.EditorFor(m => m.EmailCli)
        </div>

        <div class="editor-label">
            @Html.LabelFor(m => m.TelCli)
        </div>
        <div class="editor-field">
            @Html.EditorFor(m => m.TelCli)
        </div>

        <div class="editor-label">
            @Html.LabelFor(m => m.DataCli)
        </div>
        <div class="editor-field">
            @Html.EditorFor(m => m.DataCli)
        </div>
        <div class="editor-label">
            @Html.LabelFor(m => m.GeneCli)
        </div>
        <div class="editor-field">
            @Html.EditorFor(m => m.GeneCli)
        </div>

        <br />

        


        <p>
            <input type="submit" value="Enviar" />
        </p>
    </fieldset>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
